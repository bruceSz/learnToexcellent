class qObj():
    _q = None
    o = None
    def __init__(self,dQ,autoGet = False):
        self._q = dQ
        if autoGet == True:
            self.o = self._q.get()
    def __enter__(self):
        if self.o == None:
            self.o = self._q.get()
        return self.o
    
    def __exit__(self,type,value,traceback):
        if self.o != None:
            self._q.put(self.o)
