""" basic tody list using webpy """
import web
import model

urls= (
    '/','index',
    '/del/(\d+)','delete'
)
render=web.template.render('templates',base='base')

class index:
    form = web.form.Form(
        web.form.Textbox('title',web.form.notnull,
